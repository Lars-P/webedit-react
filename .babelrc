{
  "presets": [["env", {
      "targets": {
        "chrome": 45,
        "firefox": 42,
        "safari": 9,
        "opera": 32,
        "edge": 12,
        "iOS": 9
      },
      "loose": true,
      "useBuiltIns": true
      }],
      "react",
      "stage-0"
    ],
  "plugins": [
    "transform-decorators-legacy"
  ],
  "env": {
    "production": {
      "plugins": [
        "transform-remove-console",
        "transform-remove-debugger"
      ]
    }
  }
}
